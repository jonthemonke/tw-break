    },
    "xs3-keyword": {
      "match": "xs3:",
      "name": "twbreak.xs3"
    },
    "sm-keyword": {
      "match": "sm:",
      "name": "twbreak.sm"
    },
    "sm1-keyword": {
      "match": "sm1:",
      "name": "twbreak.sm1"
    },
    "sm2-keyword": {
      "match": "sm2:",
      "name": "twbreak.sm2"
    },
    "sm3-keyword": {
